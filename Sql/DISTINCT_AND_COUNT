SELECT DISTINCT replacement_cost FROM FILM;

//replacement_cost sütünunda birbirinden farklı olan verileri getirir.

SELECT COUNT (DISTINCT replacement_cost)FROM FILM;

//replacement_cost sütünunda birbirinden farklı kaç tane değer olduğunun sayısını verir.

SELECT COUNT (DISTINCT title) FROM film WHERE title LIKE 'T%' AND rating= 'G';

//film tablosundaki title sütünunda T ile başlayan ve ratingi 'G'ye eşit olan değerlerin toplam sayısını verir.

SELECT COUNT(*) FROM country WHERE LENGTH (country)=5;

//country tablosundaki ülkelerden kaç tanesinin 5 harften oluştuğunu gösterir.

SELECT COUNT(*) FROM city WHERE city ILIKE '%R';

//city toablosundaki isimlerin kaç tanesinin 'r' veya 'R' ile bittiğini gösterir.
